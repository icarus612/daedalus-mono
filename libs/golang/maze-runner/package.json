{
  "name": "lib.golang.maze-runner",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "go build -o bin/cli-solver ./cmd/cli-solver && go build -o bin/quick-solver ./cmd/quick-solver",
    "dev": "~/go/bin/gotestsum --watch ./...",
    "test": "go test ./...",
    "lint": "go vet ./... && go fmt ./..."
  },
  "bin": {
    "cli-solver": "bin/cli-solver",
    "quick-solver": "bin/quick-solver"
  }
}
